"MonitorScreen"
{
	"$baseTexture"			"zerochain/props_arcade/coinpusher/zap_coinpusher_lights02"
	
	"$surfaceprop" "Glass"
	"$model" 1
	
	"$additive" 0
	"$envmapmask"		"zerochain/props_arcade/coinpusher/zap_coinpusher_lights02_illummask"
	"$selfillum_envmapmask_alpha" "1"
	"$selfillumtint" "[1 1 1]"
	
	"$color2" "[0.1 1 0]"
	
	"$blendtintbybasealpha" "1"
	"$blendtintcoloroverbase" "0"
	
	"Proxies"
        {
            "TextureScroll"
            {
                "textureScrollVar" "$basetexturetransform"
                "textureScrollRate" 1
                "textureScrollAngle" 90
            }
			
			"AnimatedTexture"
			{
			"animatedTextureVar" "$basetexture"
			"animatedTextureFrameNumVar" "$frame"
			"animatedTextureFrameRate" "1"
			}
        }
}
